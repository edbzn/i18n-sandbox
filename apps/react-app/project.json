{
  "name": "react-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/react-app/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "extract-i18n": {
      "executor": "@i18n-sandbox/i18n-tools:extract-translations",
      "options": {
        "sourceRoot": "apps/react-app/src",
        "outputPath": "apps/react-app/src/i18n",
        "pattern": "\\$localize`:@@([^:]+):[^`]*`"
      }
    }
  }
}
